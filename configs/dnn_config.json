{
  "exp": {
    "name": "dnn_512_384_256",
    "num_of_classes": 101
  },
  "model":{
    "optimizing": {
      "learning_rate": 0.001,
      "decay": 1e-6,
      "optimizer": "adam"
    },
    "architecture": {
      "available": false,
      "dense": [
        2048,
        1024
      ]
    }
  },
  "trainer":{
    "num_epochs": 5,
    "batch_size": 512,
    "verbose_training": true,
    "train_split": "train01.txt",
    "test_split": "validation01.txt"
  },
  "callbacks":{
    "save_every_n": 1,
    "checkpoint_monitor": "val_acc",
    "checkpoint_mode": "max",
    "checkpoint_save_best_only": true,
    "checkpoint_save_weights_only": true,
    "checkpoint_verbose": true,
    "tensorboard_write_graph": true,
    "early_stopping": true
  }
}